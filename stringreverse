public static void main(String[] args) {
	

	String str = "this is programming class";]
  	String[] str1 = str.split(" ");
	for(int i = str1.length-1; i>=0;i--)
	{
		if (i!=2)
		{
			System.out.println(str1[i]);

		}
		else
		{
			for(int j = str1[2].length()-1;j>=0;j--)
			{
				System.out.print(str1[2].charAt(j));
			}
			System.out.println();
		}
	}
}
